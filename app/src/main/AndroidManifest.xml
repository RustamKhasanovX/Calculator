        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Calculator.">
        <activity
            android:name="ru.khasanov.calculator1.presentation.main.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="ru.khasanov.calculator1.presentation.settings.SettingsActivity" />
        <activity android:name="ru.khasanov.calculator1.presentation.history.HistoryActivity" />
    </application>
    <uses-permission android:name="android.permission.VIBRATE" />
</manifest>